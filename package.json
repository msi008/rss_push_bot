{
  "name": "rsshub-enterprise-wechat-bot",
  "version": "1.0.0",
  "description": "订阅RSSHub源并推送到企业微信机器人",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "lint": "eslint src/",
    "build": "echo '构建成功，无需编译步骤'",
    "deno:install": "npm install --ignore-scripts",
    "deno:start": "deno run --allow-net --allow-read --allow-env --allow-write src/app.js",
    "no-python:install": "npm install --ignore-scripts",
    "no-python:start": "node src/app.js"
  },
  "keywords": [
    "rsshub",
    "wechat",
    "rss",
    "notification",
    "bot"
  ],
  "author": "RSS推送服务",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3",
    "rss-parser": "^3.13.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "wrangler": "^4.47.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "deno": {
    "nodeModulesDir": true
  }
}
